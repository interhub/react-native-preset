{
  "name": "stayfitt",
  "displayName": "stayfitt",
  "expo": {
    "name": "stayfitt",
    "slug": "stayfitt",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "ios": {
