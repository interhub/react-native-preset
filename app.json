    "androidNavigationBar": {
      "backgroundColor": "#DCF763",
      "barStyle": "dark-content"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    }
